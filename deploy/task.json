{
  "family": "replicated-installer",
  "networkMode": "bridge",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "EC2"
  ],
  "containerDefinitions": [
    {
      "essential": true,
      "memoryReservation": 64,
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 80,
          "hostPort": 0
        }
      ],
      "environment": [
        {
          "name": "USE_EC2_PARAMETERS",
          "value": "1"
        },
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        },
        {
          "name": "PROJECT_NAME",
          "value": "vendor-api"
        }
      ],
      "name": "replicated-installer",
      "mountPoints": [],
      "image": "${DOCKER_IMAGE}",
      "logConfiguration": {
        "logDriver": "json-file"
      },
      "volumesFrom": []
    }
  ]
}